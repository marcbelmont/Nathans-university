start =
    expression
validchar
    = [0-9a-zA-Z_?!+-=@#$%^&*/.]

atom =
    chars:validchar+
        { return chars.join(""); }
space = [ \t\n]
spacedexpression =
	space+ e:expression { return e; }
expression =
	atom
    / space* "(" space* a:atom s:spacedexpression* space* ")" space*
    { return [a].concat(s); }
